{
  "name": "ui-prototype",
  "private": false,
  "version": "1.0.0",
  "description": "Modern React + TypeScript + Material-UI web application with authentication, internationalization, testing, and Figma MCP integration",
  "keywords": [
    "react",
    "typescript",
    "material-ui",
    "vite",
    "tanstack-query",
    "msw",
    "figma-mcp",
    "i18n",
    "authentication",
    "clean-architecture",
    "playwright",
    "e2e",
    "testing",
    "mcp",
    "ai-assisted"
  ],
  "author": "nakai95",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nakai95/ui-prototype.git"
  },
  "homepage": "https://github.com/nakai95/ui-prototype#readme",
  "bugs": {
    "url": "https://github.com/nakai95/ui-prototype/issues"
  },
  "type": "module",
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "type-check": "tsc --project tsconfig.app.json --noEmit",
    "lint": "eslint . --max-warnings 0",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:related": "vitest related --run",
    "test:e2e": "playwright test --config=playwright/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=playwright/playwright.config.ts --ui",
    "test:e2e:debug": "playwright test --config=playwright/playwright.config.ts --debug",
    "test:e2e:headed": "playwright test --config=playwright/playwright.config.ts --headed",
    "test:e2e:chromium": "playwright test --config=playwright/playwright.config.ts --project=chromium",
    "test:e2e:firefox": "playwright test --config=playwright/playwright.config.ts --project=firefox",
    "test:e2e:webkit": "playwright test --config=playwright/playwright.config.ts --project=webkit",
    "test:e2e:mobile": "playwright test --config=playwright/playwright.config.ts --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:e2e:report": "playwright show-report playwright/playwright-report",
    "test:e2e:install": "playwright install --with-deps",
    "gen:api": "orval --config ./schema/orval.config.ts",
    "msw:init": "msw init public/ --save"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/x-tree-view": "^8.5.1",
    "@tanstack/react-query": "^5.80.6",
    "axios": "^1.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.65.0",
    "react-router-dom": "^7.6.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@faker-js/faker": "^9.8.0",
    "@playwright/test": "^1.56.1",
    "@tanstack/react-query-devtools": "^5.80.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "3.2.2",
    "@vitest/ui": "^3.2.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.25.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsdom": "^26.1.0",
    "msw": "^2.9.0",
    "orval": "^7.10.0",
    "prettier": "^3.5.3",
    "react-i18next": "^15.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.2"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
